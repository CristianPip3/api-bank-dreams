{
  "name": "service-account-bank-dreams",
  "version": "1.0.0",
  "description": "Service about accounts and transactions of user",
  "main": "index.js",
  "scripts": {
    "start": "NODE_PATH=. NODE_ENV=production node index.js",
    "dev": "cross-env NODE_PATH=. NODE_ENV=development nodemon",
    "migrate:dev": "NODE_ENV=development sequelize db:migrate",
    "migrate:undo": "NODE_ENV=development sequelize db:migrate:undo:all",
    "seed:dev": "cross-env NODE_ENV=development NODE_PATH=. sequelize db:seed:all --seeders-path=./src/infrastructure/sequelize/seeders/development",
    "seed:undo": "cross-env NODE_ENV=development NODE_PATH=. sequelize db:seed:undo:all --seeders-path=./src/infrastructure/sequelize/seeders/development"
  },
  "keywords": [
    "Node",
    "Awilix",
    "Passport"
  ],
  "author": "CristianPip3 <cristianbenavides@unicauca.edu.co>",
  "license": "ISC",
  "dependencies": {
    "awilix": "^4.3.1",
    "awilix-express": "^3.0.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-status-monitor": "^1.3.3",
    "faker": "^5.1.0",
    "helmet": "^4.2.0",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "prettier": "^2.2.1",
    "ramda": "^0.27.1",
    "sequelize": "^5.22.3",
    "swagger-jsdoc": "^6.0.0-rc.5",
    "swagger-ui-express": "^4.1.6",
    "tcomb": "^3.2.29",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "codecrumbs": "^1.7.3",
    "commitizen": "^4.1.2",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "docsify-cli": "^4.4.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "lint-staged": "^10.2.11",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.10",
    "pre-commit": "^1.2.2",
    "semantic-release": "^17.1.1",
    "sequelize-cli": "^5.4.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "snazzy": "^8.0.0",
    "standard": "^14.3.4",
    "supertest": "^3.4.2"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "logs/*",
      "redis-data/*",
      "postgres-data/*",
      "coverage/*"
    ],
    "delay": 1000
  },
  "standard": {
    "ignore": [
      "/public/docs"
    ],
    "globals": [
      "request",
      "app",
      "expect",
      "describe",
      "it",
      "config",
      "beforeEach"
    ]
  },
  "resolutions": {
    "extend": "^3.0.2"
  }
}
